  <RegexExpressions>
    <RegexExpression IsEnabled="true" IsForTvShow="true">^((?&lt;series_name&gt;.+?)[. _-]+)?s(?&lt;season_num&gt;\d+)[. _-]*e(?&lt;ep_num&gt;\d+)(([. _-]*e|-)(?&lt;extra_ep_num&gt;(?!(1080|720)[pi])\d+))*[. _-]*((?&lt;extra_info&gt;.+?)((?&lt;![. _-])-(?&lt;release_group&gt;[^-]+))?)?$</RegexExpression>
